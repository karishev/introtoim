void setup() {
  size(500,500);
  background(255);
  float i = 10;
  while(i<500) {
    curve(100,300, 250, 0+i, 250-i, 500, 400,300);
    i+=10;
    
  }
  i = 10;
  while(i<250) {
    strokeWeight(2);
    curve(100,300, 250, 0+i, 500, 250-i, 400,300);
    curve(100,300, 250+i, 250, 500-i, 500, 400,300);
    i+=10;
  }
  for (int j = 10; j<250; j+=10){
    curve(376, 243,0,0+j,500-j,500,123,473);
    curve(376, 243,0+j,0,500-j,500-j,123,473);
    rect(300-j,574-j, 123+j, 234-j);
    rect(563-j, 123+j, 123+j, 435-j);
  }
  for (int k = 0; k<200; k+=10){
    ellipse(440-k,440+k,100-k, 100-k);
  }
}
